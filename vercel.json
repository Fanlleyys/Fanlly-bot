{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/webhook",
            "dest": "/api/webhook.js"
        },
        {
            "src": "/api/cron/reminders",
            "dest": "/api/cron/reminders.js"
        }
    ]
}